      "source": [
        " \n",
        "x_0=2 # tha algorithm starting value\n",
        "eta=0.01 # learing rate\n",
        "epo=0.000001\n",
        "del_x=1\n",
        "Mx_iters=1000 #maximum  number of iterations \n",
        "iters=0 # iterations counter\n",
        "def derive(x):\n",
        "   x_derive=4*((x)**3)+6*x\n",
        "   return x_derive\n",
        " \n",
        "while abs(del_x)>epo and iters<Mx_iters :\n",
        "  prev_x=x_0\n",
        "  del_x=-eta*derive(prev_x)\n",
        "  x_0=x_0+del_x # gradient descent\n",
        "  iters=iters+1 # iteration count\n",
        "  print(\"iteration\",iters,\"\\n X Value is\",x_0)\n",
        " \n",
        " \n",
        "print(\"local min occurs at \",x_0)"
      ],
